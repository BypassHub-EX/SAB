-- =======================================================
-- MOVEMENT (AIRWALK + BOOSTS)
-- =======================================================
RunService.Heartbeat:Connect(function()
    if AbilityManager.Player.AirWalk and LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass(XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('cLFLpINhBUKeMwUnlRjMawqjpuEyOUNYSFMYhciHxhvsqdNSDsFLZNpSHVtYW5vaWQ=')) then
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
            LocalPlayer.Character:TranslateBy(Vector3.new(0, 0.5, 0)) -- AirWalk upward
        end
    end

    if AbilityManager.Player.JumpBoost and LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass(XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('TAUePNXUDFOAcbyJsJTQESSLzybcRvigZanNtBoVSevkVTEYKugeUpdSHVtYW5vaWQ=')) then
        local humanoid = LocalPlayer.Character:FindFirstChildOfClass(XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('vNZmQcKLWMywePXADIuwsDtTcqJIaSigbucMxgtsKxTyRBudzDmqUlxSHVtYW5vaWQ='))
        humanoid.UseJumpPower = true
        humanoid.JumpPower = math.min(AbilityManager.Player.JumpBoostValue, 175)
    end

    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass(XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('IAHYAbClKwcKzLojhIoUnxvYKgHnhLyiGTTaiSQSpPIAMMgzdxRWOkWSHVtYW5vaWQ=')) then
        LocalPlayer.Character:FindFirstChildOfClass(XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('LAjsOAuNDGoKVRwPlmtRoiAUtFlgIBGXRejdJBhhmXggESiEjlklSqxSHVtYW5vaWQ=')).WalkSpeed = AbilityManager.Player.SpeedBoost
    end
end)

