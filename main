KeyTab:CreateButton({
    Name = XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('KCXdgXtFfLYlLhlDWMpGeNhhIFEPxCLooGxsXDrFQyjWKTaWzHrUSDxVmFsaWRhdGUgS2V5'),
    Callback = function()
        if validateBusy or hubLoaded then return end
        validateBusy = true
        if currentKey == XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('xLXNKCxbuUCgZNvTNZFqexdxADLDZOhKhQVqRKizkHWRCZtODymWMqT') then
            Rayfield:Notify({ Title = XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('ucfMopyAVdMyheNhVtQuCssakiDlqQKAvmhCzNFXNYkDalRVZNrTRcmS2V5'), Content = XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('OgPaexqnEyuwalZiQYFKipOzaGKIgTSokHDZXACLuqLnhLYsfcVabEgUGxlYXNlIGVudGVyIGEga2V5Lg=='), Duration = 3 })
            validateBusy = false
            return
        end
        if isValidKey(currentKey) then
            statusLabel:Set(XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('EilwrkmIcpHjrnwJneBNOdZKQgTDvTXSzMPCFzmscFylbloVhDWoxKyU3RhdHVzOiDinIUgVmFsaWQ='))
            Rayfield:Notify({ Title = XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('wxRWaUHPmSTYLDpdIMHiZcYYqsQZqkRrNqmOnzalRuxienIrpOoJUFcS2V5'), Content = XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('QWtsrdCqeUelexynWGexkpFYamOThsZHNRpjOiuimOVjFjhRCTGEeHOS2V5IGFjY2VwdGVkLg=='), Duration = 3 })
            saveCachedKey(currentKey)
            loadHubTabs()
            KeyTab.TabFrame.Visible = false
            HubWindow:SelectTab(1)
        else
            statusLabel:Set(XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('tBMgMOFeQyEqUQlXOqPnDqIPbbWTNRJXYVIJovbPQfYsSaXDuyTtPYCU3RhdHVzOiDinYwgSW52YWxpZA=='))
            Rayfield:Notify({ Title = XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('nhmraVzpvDkTnnuwyjuZiAyaDJTJEMxZOJzOhSLFqtNuRRJycLrYsvOS2V5'), Content = XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('jiCaWdRsraKDiAgXYOJRoXWFJhjESbuHOBWuIgjriiAhPQqkCscrAIoSW52YWxpZCBrZXku'), Duration = 3 })
        end
        validateBusy = false
    end
})
