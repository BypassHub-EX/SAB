-- =======================================================
-- KEY HELPERS
-- =======================================================
local function fetchKeys()
    local ok, body = pcall(function() return game:HttpGet(KEY_URL, true) end)
    if not ok or not body then return {} end
    local keys = {}
    for line in string.gmatch(body, XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('bWQsYQGdNkQHKloRMubrmuPbpFeUjOcmPEKDTsjrmmHLnCFXuGbdoMeW15cclxuXSs=')) do
        local key = string.gsub(line, XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('LOjVMuSZbQEkVVaBRVXTkNbfokcxfBAtPOAABpKMTcySRMorHMinzyFXiVzKiguLSklcyok'), XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('qNIcEsCrUpERzbXQpfCSVHRiqdRelSetMTYGbpBdfDMThXmvMjorlbnJTE='))
        if key ~= XUfamzQGnvXIACeXkUQLcWNwVgBjRVvbpyiFOudrJFEGPiLSRgvfppZsIBielFnesHoslzEfc('jpRXYsZVnjTFrLERjKwSdDCvianAyBLlnPJrowrmBrFtoEUIypugIdt') then table.insert(keys, key) end
    end
    return keys
end

local function isValidKey(userKey)
    if not userKey or #userKey == 0 then return false end
    for _, k in ipairs(fetchKeys()) do
        if userKey == k then return true end
    end
    return false
end

local function saveCachedKey(key)
    if writefile then pcall(function() writefile(CACHE_FILE, key) end) end
end

local function readCachedKey()
    if readfile and isfile and isfile(CACHE_FILE) then
        local ok, val = pcall(function() return readfile(CACHE_FILE) end)
        if ok and val and #val > 0 then return val end
    end
    return nil
end

